{
    "name": "shimoning/freee-sdk",
    "description": "Library for Freee sdk",
    "keywords": [
        "sdk",
        "freee",
        "webhook"
    ],
    "type": "library",
    "version": "0.0.1",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7",
        "guzzlehttp/psr7": "^2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2 | ^3",
        "laravel/framework": "^9 | ^10",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.1",
        "phpmd/phpmd": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Shimoning\\FreeeSdk\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs:check": "php-cs-fixer fix --config=./php_cs.dist --dry-run --diff --verbose",
        "cs:fix": "php-cs-fixer fix --config=./php_cs.dist --verbose",
        "md": "phpmd src text phpmd.xml",
        "stan": "phpstan analyse"
    }
}
